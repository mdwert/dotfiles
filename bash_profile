PATH=$PATH:/sbin
export PATH
alias eclipse=/opt/eclipse/eclipse
alias eclipse35=/opt/eclipse35/eclipse

############################Various Aliases#####################################
alias l='ls -alF'  
alias ll='ls -alF'  
alias dir='ls -la'  
alias la='ls -Fa'  
alias ld='ls -al -d * | egrep "^d"' # only subdirectories  
alias lt='ls -alt | head -20' # recently changed files  
  
alias md='mkdir -p'  
alias rd=rmdir  
alias ..='cd ..'  
alias ...='cd ../..'
